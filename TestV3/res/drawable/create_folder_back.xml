<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:id="@+id/stats_bar_graph_bar_back">
    <shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
      <corners android:radius="10dp" />
      <gradient android:angle="90" android:startColor="#C1C1C1"
      android:endColor="#F8F8F8" android:type="linear" />
    </shape>
  </item>
  <item>
    <shape android:shape="rectangle">
    <corners android:radius="10dp" />
    <stroke android:width="3dp" android:color="#bb1c325e"/>
        <solid android:color="#00000000"/>
    </shape>
  </item>
  <item>
    <shape android:shape="rectangle">
    <corners android:radius="10dp" />
    <stroke android:width="2dp" android:color="#bb3f61a4"/>
        <solid android:color="#00000000"/>
    </shape>
  </item>
</layer-list>
